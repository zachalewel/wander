<% if current_user %>
  <div id="navbar" class="collapse navbar-collapse">
    <ul class="nav navbar-nav">
      <li><a><%= button_to 'Logout', logout_path, method: :delete, class: 'btn btn-info' %></a></li>
    </ul>
  </div>
<% else %>
  <div id="navbar" class="collapse navbar-collapse">
    <ul class="nav navbar-nav">
      <li><a><%= button_to 'Sign up', new_user_path, method: :get, class: 'btn btn-info' %></a></li>
      <li><a><%= button_to 'Login', login_path, method: :get, class: 'btn btn-info' %></a></li>
    </ul>
  </div>
<% end %>
